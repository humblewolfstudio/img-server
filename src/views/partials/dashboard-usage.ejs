<div class="bg-light p-2" style="border-radius: 10px;">
  <h2>Usage</h2>
  <div class="d-flex align-items-center">
    <div class="progress flex-grow-1" role="progressbar" aria-label="Success example" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
      <div class="progress-bar bg-success overflow-visible text-dark" style="width: <%= helper.getPercentage(userInfo.currentUsage, userInfo.maxUsage) %>%">
        <%= helper.getPercentage(userInfo.currentUsage, userInfo.maxUsage) %>%
      </div>
    </div>
    <div class="max-value ms-2">/ <%=helper.bytesToMegabytes(userInfo.maxUsage)%>MB</div>
  </div>
</div>